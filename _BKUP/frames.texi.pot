#: original_texis/frames.texi:1141 original_texis/frames.texi:1481
#, no-wrap
msgid "Management Parameters"
msgstr ""

#. type: menuentry
#: original_texis/frames.texi:1141
msgid "Communicating with the window manager."
msgstr ""

#. type: subsubsection
#: original_texis/frames.texi:1141 original_texis/frames.texi:1553 original_texis/frames.texi:1554
#, no-wrap
msgid "Cursor Parameters"
msgstr ""

