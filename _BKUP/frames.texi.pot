#: original_texis/frames.texi:1141
msgid "The position of the frame on the screen."
msgstr ""

#. type: subsubsection
#: original_texis/frames.texi:1141 original_texis/frames.texi:1279 original_texis/frames.texi:1280
#, no-wrap
msgid "Size Parameters"
msgstr ""

#. type: menuentry
#: original_texis/frames.texi:1141
msgid "Frame's size."
msgstr ""

#. type: subsubsection
#: original_texis/frames.texi:1141 original_texis/frames.texi:1356 original_texis/frames.texi:1357
#, no-wrap
msgid "Layout Parameters"
msgstr ""

#. type: menuentry
#: original_texis/frames.texi:1141
msgid "Size of parts of the frame, and enabling or disabling some parts."
msgstr ""

#. type: subsubsection
#: original_texis/frames.texi:1141 original_texis/frames.texi:1443 original_texis/frames.texi:1444
#, no-wrap
msgid "Buffer Parameters"
msgstr ""

#. type: menuentry
#: original_texis/frames.texi:1141
msgid "Which buffers have been or should be shown."
msgstr ""

